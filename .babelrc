{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false,
				"useBuiltIns": "usage",
				"corejs": 3,
				"targets": "last 1 version, > 1%, maintained node versions, not dead"
			}
		]
	],
	"plugins": [
		"@babel/transform-runtime"
	],
	"env": {
		"test": {
			"plugins": [
				"istanbul"
			]
		}
	}
}
